  "scripts": {
    "start:dev": "webpack-dev-server --config config/webpack.dev.js --mode development --host froyo.almond.com",
    "start:prod": "yarn run build && node server.js",
    "start": "node --optimize_for_size --max_old_space_size=920 --gc_interval=100 server.js",
    "start:heroku": "node --optimize_for_size --max_old_space_size=920 --gc_interval=100 server.js",
    "build": "webpack --config config/webpack.prod.js --mode production",
    "workbox:inject": "workbox injectManifest workbox-config.js",
    "lint:scss": "stylelint --fix",
    "lint:ts": "eslint --ext=jsx,ts,tsx src",
    "test:unit": "jest -u --runInBand --detectOpenHandles",
    "test:commit": "yarn test:unit --coverage",
    "cy:open": "cypress open",
    "cy:run": "cypress run --browser chrome",
    "cy:open:dev": "server-test start:dev 3000 cy:open",
    "cy:e2e:prod": "yarn run cy:run",
    "analyze": "webpack --profile --json > stats.json"
